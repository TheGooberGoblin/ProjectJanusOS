          additionalProperties: true
        priority:
          type: string
          enum: [low, normal, high]
    QueryResponse:
      type: object
      required: [query_id, status]
      properties:
        query_id:
          type: string
          format: uuid
        status:
          type: string
          enum: [queued, processing, completed, failed, cancelled]
        results:
          type: array
          items:
            type: string  # symbolic entity references
        errors:
          type: array
          items:
            type: string
    CancelResponse:
      type: object
      required: [query_id, status]
      properties:
        query_id:
          type: string
          format: uuid
        status:
          type: string
          enum: [cancelled, not_found, already_completed]
    AuditEvent:
      type: object
      required: [event_id, timestamp, actor, action]
      properties:
        event_id:
          type: string
          format: uuid
        timestamp:
          type: string
          format: date-time
        actor:
          type: string
        action:
          type: string
        details:
          type: string
    AuditFilter:
      type: object
      properties:
        date_from:
          type: string
          format: date-time
        date_to:
          type: string
          format: date-time
        event_type:
          type: string
        plugin_id:
          type: string
          format: uuid

paths:
  /query:
    post:
      summary: Submit a new symbolic query or command to Janus
      security:
        - bearerAuth: []
      requestBody:
        required: true
        content:
          application/json:
            schema:
              $ref: '#/components/schemas/QueryRequest'
      responses:
        '202':
          description: Query accepted and queued
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/QueryResponse'
        '401':
          description: Unauthorized
        '429':
          description: Rate limit exceeded

  /query/{query_id}:
    parameters:
      - name: query_id
        in: path
        required: true
        schema:
          type: string
          format: uuid
    get:
      summary: Get status and results of an existing query
      security:
        - bearerAuth: []
      responses:
        '200':
          description: Current query status
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/QueryResponse'
        '404':
          description: Query not found
    delete:
      summary: Cancel a queued or running query
      security:
        - bearerAuth: []
      responses:
        '200':
          description: Cancellation result
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/CancelResponse'
        '404':
          description: Query not found

  /audit/logs:
    get:
      summary: Retrieve audit events, filtered by parameters
      security:
        - bearerAuth: []
      parameters:
        - name: date_from
          in: query
          schema:
            type: string
            format: date-time
        - name: date_to
          in: query
          schema:
            type: string
            format: date-time
        - name: event_type
          in: query
          schema:
            type: string
        - name: plugin_id
          in: query
          schema:
            type: string
            format: uuid
      responses:
        '200':
          description: Array of audit events
          content:
            application/json:
              schema:
                type: array
                items:
                  $ref: '#/components/schemas/AuditEvent'
        '401':
          description: Unauthorized

  /audit/logs/{event_id}:
    parameters:
      - name: event_id
        in: path
        required: true
        schema:
          type: string
          format: uuid
    get:
      summary: Fetch detailed information for a single audit event
      security:
        - bearerAuth: []
      responses:
        '200':
          description: Audit event details
          content:
            application/json:
              schema:
                $ref: '#/components/schemas/AuditEvent'
        '404':
          description: Event not found

