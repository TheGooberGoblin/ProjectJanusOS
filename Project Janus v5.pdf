```yaml
openapi: 3.0.3
info:
  title: Janus API
  version: 0.4.0
  description: |
    Symbolic, schema-validated JSON I/O for Janus flows. 
    All payloads must pass [[schema.assert]] and [[lint_check:schema]].

servers:
  - url: https://api.janus.local
    description: Local Janus Instance

components:
  securitySchemes:
    BearerAuth:
      type: http
      scheme: bearer
      bearerFormat: JWT

  schemas:
    QueryRequest:
      type: object
      required:
        - query
      properties:
        query:
          type: string
          example: "What is the enthalpy of formation for water?"
        priority:
          type: string
          enum: [low, medium, high]
          default: medium
    QueryResponse:
      type: object
      properties:
        trace_id:
          type: string
          example: T-query-001
        results:
          type: array
          items:
            type: string
        confidence:
          type: number
          format: float
          example: 0.85
    AuditEntry:
      type: object
      properties:
        timestamp:
          type: string
          format: date-time
        trace_id:
          type: string
        actor:
          type: string
        action:
          type: string
    AuditResponse:
      type: object
      properties:
        entries:
          type: array
          items:
            $ref: "#/components/schemas/AuditEntry"
    PluginStatus:
      type: object
      properties:
        plugin_id:
          type: string
        status:
          type: string
          enum: [installed, active, error, decommissioned]
        last_checked:
          type: string
          format: date-time
    GovernanceResponse:
      type: object
      properties:
        plugins:
          type: array
          items:
            $ref: "#/components/schemas/PluginStatus"

  responses:
    UnauthorizedError:
      description: Missing or invalid auth token
      content:
        application/json:
          schema:
            type: object
            properties:
              error:
                type: string
                example: "Unauthorized"
    ValidationError:
      description: Schema or lint validation failed
      content:
        application/json:
          schema:
            type: object
            properties:
              error:
                type: string
                example: "[[lint_status: fail]] confidence not float"
              suggestions:
                type: array
                items:
                  type: string

security:
  - BearerAuth: []

paths:
  /query:
    post:
      summary: Submit a symbolic query to Janus
      operationId: submitQuery
      requestBody:
        required: true
        content:
          application/json:
            schema:
              $ref: "#/components/schemas/QueryRequest"
      responses:
        "200":
          description: Query results
          content:
            application/json:
              schema:
                $ref: "#/components/schemas/QueryResponse"
        "400":
          $ref: "#/components/responses/ValidationError"
        "401":
          $ref: "#/components/responses/UnauthorizedError"

  /audit:
    get:
      summary: Retrieve audit trail entries
      operationId: getAuditTrail
      parameters:
        - name: session_id
          in: query
          required: true
          schema:
            type: string
      responses:
        "200":
          description: List of audit entries
          content:
            application/json:
              schema:
                $ref: "#/components/schemas/AuditResponse"
        "401":
          $ref: "#/components/responses/UnauthorizedError"

  /governance/plugins:
    get:
      summary: List installed plugins and their status
      operationId: listPlugins
      responses:
        "200":
          description: Plugin governance status
          content:
            application/json:
              schema:
                $ref: "#/components/schemas/GovernanceResponse"
        "401":
          $ref: "#/components/responses/UnauthorizedError"
```

